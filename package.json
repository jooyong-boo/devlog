  },
  "dependencies": {
    "@prisma/client": "5.17.0",
    "@svgr/webpack": "^8.1.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.9",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
