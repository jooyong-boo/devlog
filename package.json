{
  "name": "devlog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --cache --write .",
    "lint": "next lint --cache --fix --max-warnings=0",
